# Database Configuration
# Copy this file to .env and update with your actual credentials

# Neon PostgreSQL Database Connection
# Format: postgresql://username:password@host/database?sslmode=require&channel_binding=require
DATABASE_URL=postgresql://neondb_owner:your_password@your-host.neon.tech/neondb?sslmode=require&channel_binding=require

# Individual connection parameters (alternative to DATABASE_URL)
DB_HOST=your-host.neon.tech
DB_PORT=5432
DB_NAME=neondb
DB_USER=neondb_owner
DB_PASSWORD=your_password
DB_SSLMODE=require
DB_CHANNEL_BINDING=require
